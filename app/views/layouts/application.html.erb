<!DOCTYPE html>
<html>
<head>
  <title>Secret</title>
  <%= stylesheet_link_tag "https://cdnjs.cloudflare.com/ajax/libs/materialize/0.97.5/css/materialize.min.css" %>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
   <%= javascript_include_tag "https://cdnjs.cloudflare.com/ajax/libs/materialize/0.97.5/js/materialize.min.js" %>
  <%= stylesheet_link_tag "http://fonts.googleapis.com/icon?family=Material+Icons" %>
  <%= csrf_meta_tags %>
</head>
<body>
<% if flash[:errors] %>
  <% flash[:errors].each do |e| %>
    <p><%= e %></p>
  <% end %>
<% end %>
 
   <nav>
    <div class="nav-wrapper teal darken-1">
      <a href="/users/<%= current_user.id%>" class="brand-logo">Secrets</a>
      <ul id="nav-mobile" class="right hide-on-med-and-down">
      <% if :current_user %>
      <li><a href="/sessions/new" class="waves-effect waves-light btn">Log Out</a></li>
<% end %>
      </ul>
    </div>
  </nav>
        


<%= yield %>
</body>
</html>